    "eslint-watch": "6.0.1",
    "file-loader": "5.1.0",
    "hard-source-webpack-plugin": "0.13.1",
    "history": "4.10.1",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "25.1.0",
    "jest-cli": "25.1.0",
    "json-loader": "0.5.7",
    "mini-css-extract-plugin": "0.9.0",
    "mockdate": "2.0.5",
    "node-sass": "^4.13.1",
    "open-cli": "5.0.0",
    "postcss-loader": "3.0.0",
    "prettier": "^1.19.1",
    "prompt": "1.0.0",
    "prop-types": "15.7.2",
