{
  "name": "Depviz",
  "version": "1.0.0",
  "description": "ðŸ‘“ dependency visualizer for GitHub & GitLab (a.k.a., 'auto-roadmap')",
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm run start-message",
    "start": "npm run open:src",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist",
    "build": "babel-node tools/build.js",
    "test": "jest --passWithNoTests",
    "test:CI": "jest --passWithNoTests",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "jest --watchAll",
    "open:cover": "npm run test:cover && open-cli ./coverage/lcov-report/index.html",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js"
  },
  "dependencies": {
    "@hot-loader/react-dom": "16.13.0",
    "bitmap2vector": "0.0.4",
    "blob-to-buffer": "1.2.8",
    "core-js": "3.6.5",
    "cytoscape": "3.14.2",
    "cytoscape-cola": "2.3.1",
    "cytoscape-elk": "valohai/cytoscape.js-elk#modern",
    "cytoscape-node-html-label": "1.2.0",
    "html2canvas": "1.0.0-rc.5",
    "mermaid": "8.5.0",
    "potrace": "2.1.6",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-draggable": "4.3.1",
    "react-feather": "2.0.8",
    "react-hook-form": "6.0.1",
    "react-router-dom": "5.2.0",
    "tabler": "1.0.0-alpha.7"
  },
  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.11.4",
    "@babel/node": "7.10.5",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-react-constant-elements": "7.10.4",
    "@babel/plugin-transform-runtime": "7.11.0",
    "@babel/polyfill": "7.10.4",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-react": "7.10.4",
    "autoprefixer": "9.7.6",
    "babel-eslint": "10.1.0",
    "babel-jest": "25.5.1",
    "babel-loader": "8.1.0",
    "babel-plugin-add-module-exports": "1.0.2",
    "babel-plugin-inline-import": "3.0.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "browser-sync": "2.26.7",
    "chalk": "4.0.0",
    "connect-history-api-fallback": "1.6.0",
    "coveralls": "3.1.0",
    "cross-env": "7.0.2",
    "css-loader": "3.5.3",
    "cssnano": "4.1.10",
    "dotenv-webpack": "2.0.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "7.0.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "4.0.8",
    "eslint-watch": "7.0.0",
    "file-loader": "6.0.0",
    "history": "5.0.0",
    "html-webpack-plugin": "4.2.0",
    "jest": "25.5.4",
    "jest-cli": "25.5.4",
    "json-loader": "0.5.7",
    "mini-css-extract-plugin": "0.9.0",
    "mockdate": "3.0.2",
    "node-sass": "4.14.0",
    "open-cli": "6.0.1",
    "postcss-loader": "3.0.0",
    "prettier": "2.0.5",
    "prompt": "1.0.0",
    "prop-types": "15.7.2",
    "raf": "3.4.1",
    "react-hot-loader": "4.12.20",
    "react-test-renderer": "16.13.1",
    "replace": "1.2.0",
    "rimraf": "3.0.2",
    "sass-loader": "9.0.1",
    "stats.js": "0.17.0",
    "style-loader": "1.2.0",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "webpack-bundle-analyzer": "3.7.0",
    "webpack-dev-middleware": "3.7.2",
    "webpack-hot-middleware": "2.25.0"
  },
  "keywords": [
    "react",
    "reactjs",
    "react-router",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "boilerplate",
    "starter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/moul/depviz"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/assetsTransformer.js",
      "\\.(css)$": "<rootDir>/tools/assetsTransformer.js"
    },
    "setupFiles": [
      "raf/polyfill",
      "./tools/enzymeTestAdapterSetup.js"
    ]
  }
}
